<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.suitedcoffee.coffeeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Questionnaire"
            android:label="Suiting The Right Coffee For You"></activity>
        <activity android:name=".CoffeeShopActivity"
            android:label="List Of Coffee Shops"></activity>
        <activity android:name=".SearchActivity"
            android:label="Searching The Best For You"
            ></activity>
        <activity android:name=".AboutUs"
            android:label="About Us"></activity>
        <activity android:name=".ContactUs"
            android:label="Contact Us"></activity>

    </application>

</manifest>